<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>
<%= link_to "新增项目", new_project_path %>

<table class="table">
  <thead>
    <tr>
      <td>项目名</td>
      <td>项目发起人</td>
      <td>项目金额</td>
      <td>處理</td>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
    <tr>
      <td><%= project.name %></td>
      <td><%= project.sponsor%></td>
      <td><%= project.amount %></td>
      <td>
        <%= link_to "編輯", edit_project_path(project) %>

      </td>
    </tr>
    <% end %>
  </tbody>
</table>

